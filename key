echo -ne 'qual a senha?: '; read key
case "$key" in
    01010-78901)
   clear
   apt-get update -y; apt-get upgrade -y; wget https://raw.githubusercontent.com/GuerraVPN/VPSScripts/main/insvpss.sh; chmod 777 insvpss.sh; ./insvpss.sh; rm insvpss.sh
   clear
   ;;
    *)
   echo Senha Invalida
   exit;
   ;;
esac
